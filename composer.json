{
    "name": "lwo/lwo-bootstrap-news",
    "type": "typo3-cms-extension",
    "description": "Bootstrap 5 Package compatible News Templates",
    "license": [
        "GPL-2.0+"
    ],
    "require": {
        "typo3/cms-core": " ^11.5 || main-dev",
        "georgringer/news": "^9.4"
    },
    "suggest": {
        "bk2k/bootstrap-package": "Bootstrap Package with Bootstrap 5 enabled"
    },
    "autoload": {
        "psr-4": {
            "LWO\\LwoBootstrapNews\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "lwo_bootstrap_news"
        }
    }
}
